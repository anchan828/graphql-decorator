{
  "name": "graphql-decorator",
  "version": "0.2.0",
  "description": "Creates GraphQL Schema from decorated TypeScript Class. [Note] this package forked from https://github.com/Quramy/graphql-decorator",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4.4.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run build && npm run lint && mocha lib/**/*.spec.js",
    "watch": "tsc -w",
    "watch-test": "mocha lib/**/*.spec.js --watch"
  },
  "keywords": [
    "graphql",
    "typescript",
    "decorators"
  ],
  "author": "Quramy",
  "contributors": [
    "anchan828"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anchan828/graphql-decorator.git"
  },
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.11.7",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.44",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "typings": "lib/index.d.ts"
}
